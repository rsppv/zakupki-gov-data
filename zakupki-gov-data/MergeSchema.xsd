			<xs:enumeration value="RA"/>
			<xs:enumeration value="ORA"/>
			<xs:enumeration value="AI"/>
			<xs:enumeration value="OAI"/>
			<xs:enumeration value="OA"/>
			<xs:enumeration value="OOA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_schemeVersionType">
		<xs:annotation>
			<xs:documentation>Версия схемы</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1.0"/>
			<xs:enumeration value="4.1"/>
			<xs:enumeration value="4.2"/>
			<xs:enumeration value="4.3"/>
			<xs:enumeration value="4.3.100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_settlementAccountType">
		<xs:annotation>
			<xs:documentation>Расчетный счет</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_signatureType">
		<xs:annotation>
			<xs:documentation>Тип подписи</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CAdES-BES"/>
			<xs:enumeration value="CAdES-A"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_sketchPlanNumberType">
		<xs:annotation>
			<xs:documentation>Реестровый номер плана закупок</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_spzNumType">
		<xs:annotation>
			<xs:documentation>Код по СПЗ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{11}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_subordinationTypeEnum">
		<xs:annotation>
			<xs:documentation>Виды подчиненности организаций</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_subsystemTypeDTEnum">
		<xs:annotation>
			<xs:documentation>Виды подсистем для неструктурированных документов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FZ44"/>
			<xs:enumeration value="ISO"/>
			<xs:enumeration value="ISM"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_subsystemTypeEnum">
		<xs:annotation>
			<xs:documentation>Виды подсистем</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FZ94"/>
			<xs:enumeration value="FZ44"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_tenderPlan.PurchasePlacingType">
		<xs:annotation>
			<xs:documentation>Способ определения поставщиков(подрядчиков, исполнителей)
			
1 - открытый конкурс;
2 - конкурс с ограниченным участием;
3 - двухэтапный конкурс;
4 - закрытый конкурс;
5 - закрытый конкурс с ограниченным участием;
6 - закрытый двухэтапный конкурс;
7 - аукцион в электронной форме;
8 - закрытый аукцион;
9 - запрос котировок;
10 - запрос предложений; 
11 - закупка у единственного поставщика (подрядчика, исполнителя); 12- иной способ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="7"/>
			<xs:enumeration value="8"/>
			<xs:enumeration value="9"/>
			<xs:enumeration value="10"/>
			<xs:enumeration value="11"/>
			<xs:enumeration value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_tenderPlanNumberType">
		<xs:annotation>
			<xs:documentation>Реестровый номер плана-графика</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_tenderPlanPositionNumberType">
		<xs:annotation>
			<xs:documentation>Реестровый номер позиции в плане-графике</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_tenderSuspensionActionType">
		<xs:annotation>
			<xs:documentation>Вид приостановки размещения закупки</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SUSPENSION"/>
			<xs:enumeration value="REOPENING"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_unplannedCheckNumberType">
		<xs:annotation>
			<xs:documentation>Номер внеплановой проверки</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_urlType">
		<xs:annotation>
			<xs:documentation>Ссылка</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_userLinkBlockStatusType">
		<xs:annotation>
			<xs:documentation>Вид блокировки связи пользователя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="BO"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_userStatusType">
		<xs:annotation>
			<xs:documentation>Вид блокировки пользователя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="BO"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_valueType">
		<xs:annotation>
			<xs:documentation>Значимость критерия/показателя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="5"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_violationLevelType">
		<xs:annotation>
			<xs:documentation>Тип нарушения в протоколе приема</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="error"/>
			<xs:enumeration value="warning"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_yearType">
		<xs:annotation>
			<xs:documentation>Номер года</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1900"/>
			<xs:maxInclusive value="2100"/>
			<xs:pattern value="\d{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:group name="delivery">
		<xs:annotation>
			<xs:documentation>Срок и место поставки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deliveryTerm">
				<xs:annotation>
					<xs:documentation>Срок поставки товара, выполнения работ, оказания услуг</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="deliveryPlace">
				<xs:annotation>
					<xs:documentation>Место поставки товара, выполнения работ, оказания услуг</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="energyService">
		<xs:annotation>
			<xs:documentation>Размещение заказов на энергосервис для нужд заказчиков (гл. 7.1 Федерального закона №94-ФЗ)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="energyType" type="energyService.energyType">
				<xs:annotation>
					<xs:documentation>Тип экономии

FV - Фиксированный размер экономии (п. 1 ч. 3 ст. 56.1 Закона №94-ФЗ);
FP - Фиксированный процент экономии (п. 2 ч. 3 ст. 56.1 Закона №94-ФЗ);
MIN - Минимальный размер экономии (п. 3 ч. 3 ст. 56.1 Закона №94-ФЗ)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="energyServiceEconomy">
				<xs:annotation>
					<xs:documentation>Условие экономии</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:element name="export">
		<xs:annotation>
			<xs:documentation>Схема данных подсистемы экспорта</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element minOccurs="0" name="notificationOK" type="notificationOKType">
					<xs:annotation>
						<xs:documentation>Извещение о проведении открытого конкурса;
Извещение о внесении изменений в извещение о проведении открытого конкурса или конкурсную документацию.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="notificationEF" type="notificationEFType">
					<xs:annotation>
						<xs:documentation>Извещение о проведении ЭА;
Извещение о внесении изменений в извещение о проведении ЭА или аукционную документацию.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="notificationZK" type="notificationZKType">
					<xs:annotation>
						<xs:documentation>Извещение о проведении запроса котировок;
Извещение о продлении срока подачи котировочных заявок</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="notificationPO" type="notificationPOType">
					<xs:annotation>
						<xs:documentation>Извещение о проведении предварительного отбора</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="notificationSZ" type="notificationSZType">
					<xs:annotation>
						<xs:documentation>Сообщение о заинтересованности в проведении конкурса</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="notificationCancel" type="notificationCancelType">
					<xs:annotation>
						<xs:documentation>Извещение об отказе от проведения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolOK1" type="protocolOK1Type">
					<xs:annotation>
						<xs:documentation>Протокол вскрытия конвертов с заявками на участие в открытом конкурсе</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolOK2" type="protocolOK2Type">
					<xs:annotation>
						<xs:documentation>Протокол рассмотрения заявок на участие в открытом конкурсе</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolOK3" type="protocolOK3Type">
					<xs:annotation>
						<xs:documentation>Протокол оценки и сопоставления заявок на участие в открытом конкурсе</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolEF1" type="protocolEF1Type">
					<xs:annotation>
						<xs:documentation>Протокол рассмотрения первых частей заявок на участие в открытом аукционе в электронной форме</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolEF2" type="protocolEF2Type">
					<xs:annotation>
						<xs:documentation>Протокол проведения открытого аукциона в электронной форме</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolEF3" type="protocolEF3Type">
					<xs:annotation>
						<xs:documentation>Протокол подведения итогов открытого аукциона в электронной форме</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolZK1" type="protocolZK1Type">
					<xs:annotation>
						<xs:documentation>Протокол рассмотрения котировочных заявок</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolZK5" type="protocolZK5Type">
					<xs:annotation>
						<xs:documentation>Протокол рассмотрения котировочных заявок по главе 5</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolPO1" type="protocolPO1Type">
					<xs:annotation>
						<xs:documentation>Протокол рассмотрения заявок на участие в процедуре предварительного отбора </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolEvasion" type="protocolEvasionType">
					<xs:annotation>
						<xs:documentation>Протокол отказа от заключения контракта</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="protocolCancel" type="protocolCancelType">
					<xs:annotation>
						<xs:documentation>Информация об отмене протокола</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="placementResult" type="placementResultType">
					<xs:annotation>
						<xs:documentation>Результат размещения заказа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" name="contractSign" type="contractSignType">
					<xs:annotation>
						<xs:documentation>Информация о подписании государственного/муниципального контракта на ЭП</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>