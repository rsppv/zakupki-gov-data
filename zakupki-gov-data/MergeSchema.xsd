		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="contractCount" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation>Количество контрактов</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_contractProcedureCancelType">
		<xs:annotation>
			<xs:documentation>Сведения об отмене информации об исполнении (расторжении) контракта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cancelledProcedureId">
				<xs:annotation>
					<xs:documentation>Идентификатор отменяемой информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="regNum" type="zfcs_contract.regNumType">
				<xs:annotation>
					<xs:documentation>Номер реестровой записи государственного или муниципального контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancelDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата отмены информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason">
				<xs:annotation>
					<xs:documentation>Основание отмены информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="currentContractStage" type="zfcs_contract.contractStageType">
				<xs:annotation>
					<xs:documentation>Текущее состояние контракта:
E 	Исполнение;
ET	Исполнение прекращено;
EC	Исполнение завершено;
IN	Aннулировано.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="zfcs_schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractProcedureType">
		<xs:annotation>
			<xs:documentation>Информация об исполнении (расторжении) контракта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="id">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ФКС</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long"/>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="externalId" type="zfcs_externalIdType">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regNum" type="zfcs_contract.regNumType">
				<xs:annotation>
					<xs:documentation>Номер реестровой записи государственного или муниципального контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="publishDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата публикации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="versionNumber" type="xs:int">
				<xs:annotation>
					<xs:documentation>Номер редакции сведений</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="executions">
				<xs:annotation>
					<xs:documentation>Информация об исполнении</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="stage" type="zfcs_stageType">
							<xs:annotation>
								<xs:documentation>Этап контракта</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ordinalNumber" type="xs:int">
							<xs:annotation>
								<xs:documentation>Номер исполнения в рамках этапа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="finalStageExecution" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>Признак закрытия исполнения по этапу</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" name="execution">
							<xs:annotation>
								<xs:documentation>Детализация информации об исполнении контракта</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="documentName">
										<xs:annotation>
											<xs:documentation>Наименование документа</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="500"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="documentNum">
										<xs:annotation>
											<xs:documentation>Номер документа</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="documentDate" type="xs:date">
										<xs:annotation>
											<xs:documentation>Дата документа</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="paid" type="zfcs_moneyType">
										<xs:annotation>
											<xs:documentation>Фактически оплачено в рублях</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="product">
										<xs:annotation>
											<xs:documentation>Объем поставленного товара, выполненных работ, оказанных услуг</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="4000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="quantityContractSubjects">
										<xs:annotation>
											<xs:documentation>Объем фактически поставляемого товара, объёма выполняемых работ, оказываемых услуг на основании предмета контракта из сведений о государственном или муниципальном контракте</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element maxOccurs="unbounded" name="quantityContractSubject">
													<xs:complexType>
														<xs:sequence>
															<xs:element minOccurs="0" name="sid">
																<xs:annotation>
																	<xs:documentation>Уникальный идентификатор предмета контракта в ФКС</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:long"/>
																</xs:simpleType>
															</xs:element>
															<xs:element minOccurs="0" name="quantity" type="xs:double">
																<xs:annotation>
																	<xs:documentation>Объем фактически поставляемого товара, объёма выполняемых работ, оказываемых услуг</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="terminations">
				<xs:annotation>
					<xs:documentation>Информация о расторжении</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="termination">
							<xs:annotation>
								<xs:documentation>Детализация информации о расторжении контракта</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="paid" type="zfcs_moneyType">
										<xs:annotation>
											<xs:documentation>Фактически оплачено Заказчиком в рублях</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="terminationDate" type="xs:date">
										<xs:annotation>
											<xs:documentation>Дата расторжения контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="reason">
										<xs:annotation>
											<xs:documentation>Причина расторжения контракта и реквизиты документа основания</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="4000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="terminationReason">
										<xs:annotation>
											<xs:documentation>Основание расторжения  контракта</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="id" type="xs:long">
													<xs:annotation>
														<xs:documentation>Идентификатор БД</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element minOccurs="0" name="name">
													<xs:annotation>
														<xs:documentation>Наименование основания</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="penalties">
				<xs:annotation>
					<xs:documentation>Информация о неустойках (штрафах, пени)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="penalty">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="contractParty" type="zfcs_contractPartyType">
										<xs:annotation>
											<xs:documentation>Сторона контракта, в отношении которой принято решение о начислении неустойки:
C - Заказчик;
S - Поставщик.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="penaltyType" type="zfcs_contractPenaltyType">
										<xs:annotation>
											<xs:documentation>Тип взыскания:
P - Неустойка;
F - Штраф;
I - Пени.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="foundation">
										<xs:annotation>
											<xs:documentation>Основание для начисления неустойки (штрафа, пени)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="400"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="penaltyAccrual">
										<xs:annotation>
											<xs:documentation>Информация о начислении неустойки</xs:documentation>
										</xs:annotation>
										<xs:complexType>
