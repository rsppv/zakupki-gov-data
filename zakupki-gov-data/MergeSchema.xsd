					<xs:documentation>Валюта контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="singleCustomerReason">
				<xs:annotation>
					<xs:documentation>Основание заключения контракта с единственным поставщиком (новые значения)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:long">
							<xs:annotation>
								<xs:documentation>Идентификатор БД</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="name" type="zfcs_nsiSingleCustomerReasonNameType">
							<xs:annotation>
								<xs:documentation>Наименование основания</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="priceChangeReason">
				<xs:annotation>
					<xs:documentation>Обоснование изменения цены контракта</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:long">
							<xs:annotation>
								<xs:documentation>Идентификатор БД</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="name">
							<xs:annotation>
								<xs:documentation>Наименование обоснования</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="comment">
							<xs:annotation>
								<xs:documentation>Комментарий</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="executionDate" type="zfcs_stageType">
				<xs:annotation>
					<xs:documentation>Срок исполнения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="finances">
				<xs:annotation>
					<xs:documentation>Финансирование</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="financeSource">
							<xs:annotation>
								<xs:documentation>Источник финансирования</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="budget">
							<xs:annotation>
								<xs:documentation>Вид бюджетных средств  (справочник бюджетов)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="code">
										<xs:annotation>
											<xs:documentation>Код бюджета</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="8"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="name">
										<xs:annotation>
											<xs:documentation>Наименование бюджета</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="2000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="budgetLevel" type="zfcs_budgetLevelType">
							<xs:annotation>
								<xs:documentation>Уровень бюджета: 
00 - бюджет не указывается;
01 - федеральный бюджет;
02 - бюджет субъекта Российской Федерации;
03 - местный бюджет;
04 - бюджет Пенсионного фонда Российской Федерации;
05 - бюджет Федерального фонда обязательного медицинского страхования;
06 - бюджет Фонда социального страхования Российской Федерации;
07 - бюджет территориального государственного внебюджетного фонда</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="budgetary">
							<xs:annotation>
								<xs:documentation>За счет бюджетных средств</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="zfcs_subStageType">
										<xs:sequence>
											<xs:element name="KBK">
												<xs:annotation>
													<xs:documentation>Код бюджетной классификации</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:length value="20"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="price" type="zfcs_moneyType">
												<xs:annotation>
													<xs:documentation>Цена контракта</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element minOccurs="0" name="comment">
												<xs:annotation>
													<xs:documentation>Примечание</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:maxLength value="1024"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="extrabudget">
							<xs:annotation>
								<xs:documentation>Вид внебюджетных средств</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="code">
										<xs:annotation>
											<xs:documentation>Код</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="name">
										<xs:annotation>
											<xs:documentation>Наименование</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="350"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="extrabudgetary">
							<xs:annotation>
								<xs:documentation>За счет внебюджетных средств</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="zfcs_subStageType">
										<xs:sequence>
											<xs:element name="KOSGU" type="xs:string">
												<xs:annotation>
													<xs:documentation>Код операций сектора государственного управления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="price" type="zfcs_moneyType">
												<xs:annotation>
													<xs:documentation>Цена контракта</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="products">
				<xs:annotation>
					<xs:documentation>Предмет контракта</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="product">
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="sid">
										<xs:annotation>
											<xs:documentation>Уникальный идентификатор в ФКС</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:long"/>
										</xs:simpleType>
									</xs:element>
									<xs:element name="OKPD" type="zfcs_OKPDRef">
										<xs:annotation>
											<xs:documentation>Классификация товаров, работ и услуг</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="name">
										<xs:annotation>
											<xs:documentation>Наименование товара, работ, услуг</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="4000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="OKEI" type="zfcs_contract.OKEIType">
										<xs:annotation>
											<xs:documentation>Единицы измерения</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="price" type="zfcs_moneyType">
										<xs:annotation>
											<xs:documentation>Цена за единицу</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="quantity" type="xs:double">
										<xs:annotation>
											<xs:documentation>Количество поставляемого товара, объёма выполняемых работ, оказываемых услуг</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="sum" type="zfcs_moneyType">
										<xs:annotation>
											<xs:documentation>Стоимость поставляемого товара, выполняемых работ, оказываемых услуг</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="suppliers">
				<xs:annotation>
					<xs:documentation>Поставщики</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="supplier" type="zfcs_participantType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="href" type="zfcs_hrefType">
				<xs:annotation>
					<xs:documentation>Гиперссылка на опубликованные сведения о контракте</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="printForm" type="zfcs_contract.printFormType">
				<xs:annotation>
					<xs:documentation>Печатная форма контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="scanDocuments" type="zfcs_contract.attachmentListType">
				<xs:annotation>
					<xs:documentation>Отсканированная копия контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="medicalDocuments" type="zfcs_contract.attachmentListType">
				<xs:annotation>
					<xs:documentation>Документы решения врачебной комиссии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="attachments" type="zfcs_contract.attachmentListType">
				<xs:annotation>
					<xs:documentation>Информация о прикрепленных документах</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="modification">
				<xs:annotation>
					<xs:documentation>Описание внесения изменений</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type" type="zfcs_contractModificationType">
							<xs:annotation>
								<xs:documentation>Тип вносимых изменений

M - Изменение контракта
C - Корректировка ошибок</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="description">
							<xs:annotation>
								<xs:documentation>Описание изменений</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="base">
							<xs:annotation>
								<xs:documentation>Реквизиты документа, являющегося основанием</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="currentContractStage" type="zfcs_contract.contractStageType">
				<xs:annotation>
					<xs:documentation>Текущее состояние контракта:
E 	Исполнение;
ET	Исполнение прекращено;
EC	Исполнение завершено;
IN	Aннулировано.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="zfcs_schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_countryRef">
		<xs:annotation>
			<xs:documentation>Ссылка на страну</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="countryCode">
				<xs:annotation>
					<xs:documentation>Цифровой код страны</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="countryFullName">
				<xs:annotation>
					<xs:documentation>Полное наименование страны</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_criterionType">
		<xs:annotation>
			<xs:documentation>Критерий оценки заявок участников</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code" type="zfcs_citerionCodeEnum">
				<xs:annotation>
					<xs:documentation>Код критерия:
CP - Цена контракта.
MC - Расходы на эксплуатацию и ремонт товаров, использование результатов работ.
TC - Стоимость жизненного цикла товара или созданного в результате выполнения работы объекта.
EN - Предложение о сумме соответствующих расходов заказчика, которые заказчик осуществит или понесет по энергосервисному контракту.
QF - Качественные, функциональные и экологические характеристики объекта закупки.
QO - Квалификация участников закупки, в том числе наличие у них финансовых ресурсов, на праве собственности или ином законном основании оборудования и других материальных ресурсов, опыта работы, связанного с предметом контракта, и деловой репутации, специалистов и иных работников определенного уровня</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="zfcs_valueType">
				<xs:annotation>
					<xs:documentation>Значимость критерия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="addInfo" type="zfcs_longTextType">
				<xs:annotation>
					<xs:documentation>Дополнительная информация о содержании и порядке оценки по критерию</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="indicators">
				<xs:annotation>
					<xs:documentation>Показатели критерия оценки </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="indicator" type="zfcs_indicatorType">
							<xs:annotation>
								<xs:documentation>Показатель критерия оценки </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="limit" type="zfcs_indicatorValueType">
				<xs:annotation>
					<xs:documentation>Предельное значение критерия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="measurementOrder" type="zfcs_measurementOrderEnum">
				<xs:annotation>
					<xs:documentation>Порядок оценки по критерию:
F - лучшим условием исполнения контракта является наибольшее значение,
L - лучшим условием исполнения контракта является наименьшее значение,
O - оценка производится по шкале оценки или другому порядку, указанному в документации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_currencyFullRef">
		<xs:annotation>
			<xs:documentation>Ссылка на валюту с цифровым кодом</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Код валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="digitalCode">
				<xs:annotation>
					<xs:documentation>Цифровой код валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name">
				<xs:annotation>
					<xs:documentation>Наименование валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_currencyRef">
		<xs:annotation>
			<xs:documentation>Ссылка на валюту</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Код валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name">
				<xs:annotation>
					<xs:documentation>Наименование валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_docCancelReasonType">
		<xs:annotation>
			<xs:documentation>Основание отмены документа</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_purchaseChangeType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_docType">
		<xs:annotation>
			<xs:documentation>Тип документа в рамках закупки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Кодовое наименование типа</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name">
				<xs:annotation>
					<xs:documentation>Наименование типа документа</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="350"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_documentRequirementType">
		<xs:annotation>
			<xs:documentation>Информация и документы, предусмотренные конкурсной документацией</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="number" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation>Порядковый номер информации или документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="zfcs_longTextType">
				<xs:annotation>
					<xs:documentation>Наименование информации или документа</xs:documentation>
				</xs:annotation>
