	</xs:complexType>
	<xs:complexType name="zfcs_contractCancelType">
		<xs:annotation>
			<xs:documentation>Информация об анулировании контракта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="regNum" type="zfcs_contract.regNumType">
				<xs:annotation>
					<xs:documentation>Номер реестровой записи государственного или муниципального контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancelDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата аннулирования контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentBase">
				<xs:annotation>
					<xs:documentation>Реквизиты документа, подтверждающего аннулирование контракта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="currentContractStage" type="zfcs_contract.contractStageType">
				<xs:annotation>
					<xs:documentation>Текущее состояние контракта:
E 	Исполнение;
ET	Исполнение прекращено;
EC	Исполнение завершено;
IN	Aннулировано.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="zfcs_schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractMultiType">
