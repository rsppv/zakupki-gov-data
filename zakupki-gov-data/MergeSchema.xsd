<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://zakupki.gov.ru/oos/types/1" xmlns="http://zakupki.gov.ru/oos/types/1" xmlns:oos="http://zakupki.gov.ru/oos/types/1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:complexType name="account">
		<xs:annotation>
			<xs:documentation>Банковский cчет организации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bankAddress">
				<xs:annotation>
					<xs:documentation>Адрес кредитной организации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bankName">
				<xs:annotation>
					<xs:documentation>Наименование кредитной организации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bik" type="bikType">
				<xs:annotation>
					<xs:documentation>БИК</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="corrAccount">
				<xs:annotation>
					<xs:documentation>Корреспондентский счет</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="paymentAccount" type="settlementAccountType">
				<xs:annotation>
					<xs:documentation>Расчетный счет</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="personalAccount" type="personalAccountType">
				<xs:annotation>
					<xs:documentation>Лицевой счет</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="admissionResults">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="admissionResult">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="protocolCommissionMember" type="commissionMemberType">
							<xs:annotation>
								<xs:documentation>Член комиссии осуществляющий оценку.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="admitted" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>Флаг допуска</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="appRejectedReason" type="appRejectedReasonType">
							<xs:annotation>
								<xs:documentation>Причины отказа в допуске</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="applicationFeaturesCorrespondence">
		<xs:sequence>
			<xs:element name="compatible" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Флаг соответствия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notificationFeature" type="notificationFeatureType">
				<xs:annotation>
					<xs:documentation>Особенность размещения заказа</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="appRejectedReasonType">
		<xs:sequence>
			<xs:element name="nsiRejectReason">
				<xs:annotation>
					<xs:documentation>Причина отказа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:long">
							<xs:annotation>
								<xs:documentation>Идентификатор</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="reason">
							<xs:annotation>
								<xs:documentation>Название</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="250"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="explanation">
				<xs:annotation>
					<xs:documentation>Объяснение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="auctionItemsType">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="auctionItem">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="sid" type="xs:long">
							<xs:annotation>
								<xs:documentation>Уникальный идентификатор в ООС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="description">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="price" type="xs:double">
							<xs:annotation>
								<xs:documentation>Начальная(максимальная) цена за единицу</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="bidType">
		<xs:sequence>
			<xs:element name="price" type="xs:double">
				<xs:annotation>
					<xs:documentation>Предложение цены</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата и время подачи ценового предложения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="increaseInitialPrice" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак ценового предложения на повышение начальной(максимальной) цены контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="childrenCriteriaType">
		<xs:sequence>
			<xs:element name="id" type="xs:long">
				<xs:annotation>
					<xs:documentation>Идентификатор подкритерия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>Наименование подкритерия</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="criterionValue" type="xs:double">
				<xs:annotation>
					<xs:documentation>Максимальное количество баллов</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="evalValue">
				<xs:annotation>
					<xs:documentation>Дополнительная информация или Содержание и порядок оценки по подкритерию
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2048"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="clarificationRequestType">
		<xs:annotation>
			<xs:documentation>Запрос на разъяснение конкурсной/аукционной документации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="notificationNumber" type="notificationNumberType">
				<xs:annotation>
					<xs:documentation>Номер извещения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regNumber">
				<xs:annotation>
					<xs:documentation>Номер запроса на разъяснение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="regDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата поступления запроса на разъяснение</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="topic">
				<xs:annotation>
					<xs:documentation>Тема запроса на разъяснение / 
						краткое описание запроса на разъяснение
				</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="participant">
				<xs:annotation>
					<xs:documentation>Участник размещения заказа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="name">
							<xs:annotation>
								<xs:documentation>Наименование (фамилия, имя, отчество)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="email" type="eMailType">
							<xs:annotation>
								<xs:documentation>Адрес электронной почты для отправки уведомлений</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="documentMetas" type="documentList">
				<xs:annotation>
					<xs:documentation>Информация о прикрепленных документах</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="clarificationType">
		<xs:annotation>
			<xs:documentation>Разъяснение конкурсной или аукционной документации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="id" type="xs:long">
				<xs:annotation>
					<xs:documentation>Идентификатор объекта ООС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notificationNumber" type="notificationNumberType">
				<xs:annotation>
					<xs:documentation>Номер извещения о торгах, запросе котировок</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="requestRegNumber">
				<xs:annotation>
					<xs:documentation>Номер запроса на разъяснение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="regNumber">
				<xs:annotation>
					<xs:documentation>Номер разъяснения</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="versionNumber" type="xs:int">
				<xs:annotation>
					<xs:documentation>Номер редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="createDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата создания</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="publishDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата публикации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="href">
				<xs:annotation>
					<xs:documentation>Гиперссылка на опубликованное разъяснение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="question">
				<xs:annotation>
					<xs:documentation>Тема запроса на разъяснение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="topic">
				<xs:annotation>
					<xs:documentation>Тема разъяснения / краткое описание разъяснения</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="documentMetas" type="documentList">
				<xs:annotation>
					<xs:documentation>Информация о прикрепленных документах</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="commissionMemberType">
		<xs:sequence>
			<xs:element minOccurs="0" name="id" type="xs:long">
				<xs:annotation>
					<xs:documentation>Идентификатор члена комиссии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>ФИО</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="role">
				<xs:annotation>
					<xs:documentation>Роль участника комиссии</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="roleId" type="xs:long">
							<xs:annotation>
								<xs:documentation>Идентификатор роли участника комиссии</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="name">
							<xs:annotation>
								<xs:documentation>Наименование роли</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="256"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="commissionType">
		<xs:annotation>
			<xs:documentation>Комиссии по размещению заказа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="regNumber" type="xs:int">
				<xs:annotation>
					<xs:documentation>Номер в реестре комиссий</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commissionName">
				<xs:annotation>
					<xs:documentation>Название комиссии</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="commissionMembers">
				<xs:annotation>
					<xs:documentation>Участники комиссии</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="commissionMember">
							<xs:annotation>
								<xs:documentation>Участник комиссии</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="id" type="xs:long">
										<xs:annotation>
											<xs:documentation>Идентификатор</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="role">
										<xs:annotation>
											<xs:documentation>Роль в комиссии</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="id" type="xs:long">
													<xs:annotation>
														<xs:documentation>Идентификатор</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="name">
													<xs:annotation>
														<xs:documentation>Наименование роли</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element minOccurs="0" name="order" type="xs:int">
													<xs:annotation>
														<xs:documentation>Порядковый номер</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element minOccurs="0" name="votes" type="xs:long">
													<xs:annotation>
														<xs:documentation>Количество голосов, получаемых участниками, которые имеют данную роль</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element minOccurs="0" name="rightVote" type="xs:boolean">
													<xs:annotation>
														<xs:documentation>Имеет право  голоса</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element minOccurs="0" name="name">
										<xs:annotation>
											<xs:documentation>Имя</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="256"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="owner" type="organizationRef">
				<xs:annotation>
					<xs:documentation>Организация, размещающая заказ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actual" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак актуальности</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="commonAttributesType">
		<xs:choice>
			<xs:element name="maxPrice">
				<xs:annotation>
					<xs:documentation>Максимальная стоимость</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="value" type="xs:double">
							<xs:annotation>
								<xs:documentation>Значение</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="lifeTime">
				<xs:annotation>
					<xs:documentation>Срок эксплуатации товара;
Срок технического обслуживания товара</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="value" type="xs:double">
							<xs:annotation>
								<xs:documentation>Значение</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="timeUnit" type="timeUnitType">
							<xs:annotation>
								<xs:documentation>Единица измерения времени

EMPTY – не указана;
YEAR – год;
QUATER – квартал;
MONTH – месяц;
WEEK – неделя;
DAY – день;
HOUR – час.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="timeRanges">
				<xs:annotation>
					<xs:documentation>Сроки поставки</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="timeRange">
							<xs:annotation>
								<xs:documentation>Сроки поставки</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ordinalNumber" type="xs:int">
										<xs:annotation>
											<xs:documentation>Порядковый номер</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="from" type="xs:double">
										<xs:annotation>
											<xs:documentation>Начало срока</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="to" type="xs:double">
										<xs:annotation>
											<xs:documentation>Окончание срока</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="timeUnit" type="timeUnitType">
							<xs:annotation>
								<xs:documentation>Еденица измерения времени

EMPTY – не указана;
YEAR – год;
QUATER – квартал;
MONTH – месяц;
WEEK – неделя;
DAY – день;
HOUR – час.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
