					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name">
				<xs:annotation>
					<xs:documentation>Наименование ЭТП</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="url">
				<xs:annotation>
					<xs:documentation>Адрес ЭП</xs:documentation>
				</xs:annotation>
